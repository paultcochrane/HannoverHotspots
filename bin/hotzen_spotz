#!/usr/bin/env python
# -*- coding: utf-8 -*-

from cmd import Cmd

class HPCommand(Cmd):
    def __init__(self):
        Cmd.__init__(self)

        self.intro = """\
Welcome to the Hannover Hotspot editor, version 0.1
Use Ctrl+D to exit, type 'help' or '?' for help.
"""

command = HPCommand()
command.prompt = "spotz_editor>"
print command.intro
print command.prompt

